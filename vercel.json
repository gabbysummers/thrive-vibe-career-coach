{
  "name": "thrive-vibe-career-coach",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "Thrive Vibe Career Coach",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0"
  },
  "functions": {
    "src/app/**": {
      "maxDuration": 30
    }
  }
}